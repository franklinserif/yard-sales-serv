{
  "name": "storefy",
  "version": "1.0.0",
  "description": "An e-commerce API with Express, Seuqelize, Postgresql, Docker",
  "main": "src/index.js",
  "author": "Franklin Rodriguez <franklinserif@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "nodemon ./src/index.js",
    "start": "pm2 ./src/index.js",
    "eslint": "eslint",
    "test": "jest",
    "migrations:generate": "sequelize-cli db:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:undo",
    "migrations:delete": "sequelize-cli db:undo:all",
    "seeders:generate": "sequelize-cli seed:generate --name",
    "seeders:run": "sequelize-cli db:seed:all",
    "seeders:revert": "sequelize-cli seed:undo",
    "seeders:delete": "sequelize-cli seed:undo:all",
    "jsdoc": "jsdoc -c jsdoc.json"
  },
  "devDependencies": {
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.1",
    "jsdoc": "^3.6.10",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.5",
    "pm2": "^5.2.0",
    "sequelize": "^6.20.1",
    "sequelize-cli": "^6.4.1"
  }
}
