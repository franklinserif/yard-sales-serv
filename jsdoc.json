{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["./src"],
    "includePattern": ".(jsx|js|ts|tsx)$",
    "excludePattern": "(node_modules|docs|postgres_data)"
  },
  "sourceType": "module",
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./jsdoc",
    "recurse": true
  }
}
